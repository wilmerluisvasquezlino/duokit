{
  "compilerOptions": {
    "target": "ES6", 
    "forceConsistentCasingInFileNames":true,
    "module": "Node16",  
    "outDir": "./dist",                                 
    "moduleResolution": "Node16",                     
    "declaration": true,                             
    "sourceMap": false,                                 
    "noEmitOnError": false,                             
    "preserveConstEnums": true,                       
    "esModuleInterop": true,                            
    "strict": true,                                      
    "skipLibCheck": true ,
    "removeComments": false                     
  },
  "include": ["src/**/*.ts","src/**/*.d.ts"],
  "exclude": ["node_modules"]
}
